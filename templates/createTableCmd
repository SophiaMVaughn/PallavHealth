aws dynamodb create-table --table-name zoomEmailTest --attribute-definitions AttributeName=username,AttributeType=S AttributeName=password,AttributeType=S AttributeName=email,AttributeType=S AttributeName=name,AttributeType=S --key-schema AttributeName=email,KeyType=HASH AttributeName=username,KeyType=RANGE AttributeName=password,KeyType=RANGE AttributeName=name,KeyType=RANGE --provisioned-throughput ReadCapacityUnits=5,WriteCapacityUnits=5